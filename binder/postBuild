#!/bin/bash
set -euo pipefail

wget -q https://github.com/davidbrochart/jupyterlab/archive/yjs_awareness.tar.gz -O jlab.tar.gz
tar zxf jlab.tar.gz
cd jupyterlab-yjs_awareness
pip install -e .
jlpm
jlpm build
